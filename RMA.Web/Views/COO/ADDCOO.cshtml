@model RMA.Web.Controllers.COO
	<div class="modal-dialog modal-lg">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal">&times;</button>
				<h4 class="modal-title">@ViewBag.Title</h4>
			</div>
			<div class="modal-body" id="ProductModalBody">

				<div class="row" style="margin-bottom:10px;">
					<div class="col-md-3">
						<label class="control-label pull-right">COO Number</label>
					</div>
					<div class="col-md-6">
						@Html.TextBoxFor(x => x.COONumber, new { @class = "form-control input-sm", placeholder = "COO Number" })
					</div>
				</div>
				<div class="row" style="margin-bottom:10px;">
					<div class="col-md-3">
						<label class="control-label pull-right">
							Consignee Name
						</label>
					</div>
					<div class="col-md-6" >
						@Html.TextBoxFor(x => x.ConsigneeName, new { @class = "form-control input-sm", placeholder = "Consignee Name" })
					</div>
				</div>
				<div class="row" style="margin-bottom:10px;">
					<div class="col-md-3">
						<label class="control-label pull-right">
							Declaration Date
						</label>
					</div>
					<div class="col-md-6">
						@Html.TextBoxFor(x => x.DeclarationDate, new { @class = "form-control input-sm", placeholder = "Declaration Date" })
					</div>
				</div>
				<div class="row" style="margin-bottom:10px;">
					<div class="col-md-3">
						<label class="control-label pull-right">
							VesselName
						</label>
					</div>
					<div class="col-md-6">
						@Html.TextBoxFor(x => x.VesselName, new { @class = "form-control input-sm", placeholder = "VesselName" })
					</div>
				</div>
				<div class="row" style="margin-bottom:10px;">
					<div class="col-md-3">
						<label class="control-label pull-right">
							Port Of Discharge
						</label>
					</div>
					<div class="col-md-6">
						@Html.TextBoxFor(x => x.PortOfDischarge, new { @class = "form-control input-sm", placeholder = "Port Of Discharge" })
					</div>
				</div>
				<div class="row" style="margin-bottom:10px;">
					<div class="col-md-3">
						<label class="control-label pull-right">
							Declarant Name
						</label>
					</div>
					<div class="col-md-6">
						@Html.TextBoxFor(x => x.DeclarantName, new { @class = "form-control input-sm", placeholder = "Declarant Name" })
					</div>
				</div>
				
			</div>
			<div class="modal-footer">
				<button type="button" onclick="SaveDeclarant()" class="btn btn-danger btn-frmt green btn-sm">
					<i class="fa fa-floppy-o"></i>&nbsp;Save
				</button>
				<button type="reset" class="btn btn-default btn-frmt btn-sm" data-dismiss="modal">
					<i class="fa fa-times"></i>Cancel
				</button>
			</div>
		</div>
	</div>

