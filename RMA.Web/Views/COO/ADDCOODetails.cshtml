@model EZY.RMAS.Contract.COODetail
<div class="modal-dialog modal-lg">
	<div class="modal-content">
		<div class="modal-header">
			<button type="button" class="close" data-dismiss="modal">&times;</button>
			<h4 class="modal-title">@ViewBag.Title</h4>
		</div>
		<div class="modal-body" id="ProductModalBody">

			<div class="row" style="margin-bottom:10px;">
				<div class="col-md-3">
					<label class="control-label pull-right">Model No</label>
				</div>
				<div class="col-md-6">
					<input type="hidden" id="DocumentNo" name="DocumentNo" />
					@Html.TextBoxFor(x => x.ModelNo, new { @class = "form-control input-sm", placeholder = "Model" })
				</div>
			</div>
			<div class="row" style="margin-bottom:10px;">
				<div class="col-md-3">
					<label class="control-label pull-right">
						Description
					</label>
				</div>
				<div class="col-md-6">
					@Html.TextBoxFor(x => x.Description, new { @class = "form-control input-sm", placeholder = "Description" })
				</div>
			</div>
			<div class="row" style="margin-bottom:10px;">
				<div class="col-md-3">
					<label class="control-label pull-right">
						Quantity
					</label>
				</div>
				<div class="col-md-4">
					@Html.TextBoxFor(x => x.Qty, new { @class = "form-control input-sm", placeholder = "Quantity" })
				</div>
			</div>
			<div class="row" style="margin-bottom:10px;">
				<div class="col-md-3">
					<label class="control-label pull-right">
						Origin
					</label>
				</div>
				<div class="col-md-4">
					@Html.TextBoxFor(x => x.Origin, new { @class = "form-control input-sm", placeholder = "Origin" })
				</div>
			</div>
		</div>
		<div class="modal-footer">
			<button type="button" id="btnSaveCOO" onclick="SaveCOODetails()" class="btn btn-danger btn-frmt green btn-sm">
				<i class="fa fa-floppy-o"></i>&nbsp;Save
			</button>
			<button type="reset" class="btn btn-default btn-frmt btn-sm" data-dismiss="modal">
				<i class="fa fa-times"></i>Cancel
			</button>
		</div>
	</div>
</div>


@section secscripts{
	<script src="@Url.Content("~/Scripts/jquery.dataTables.js")"></script>
	<script src="@Url.Content("~/Scripts/dataTables.bootstrap.js")"></script>
	<script type="text/javascript">

		
	</script>
}