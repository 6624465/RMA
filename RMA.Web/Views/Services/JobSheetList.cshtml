
@{
    ViewBag.Title = "JobSheetList";
}
@section seccss{
    <link href="@Url.Content("~/Css/dataTables.bootstrap.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Css/dataTables.responsive.css")" rel="stylesheet" />
}
<section class="content">
    <div class="box box-default">
        <div class="box-header with-border">
            <div class="row">
                <section class="content-header">
                    <ol class="breadcrumb">
                        <li><a href='@Url.Content("~/Home/Index")'><i class="fa fa-dashboard"></i>Home</a></li>
                        @*<li><a href='@Url.Content("~/Home/Index")'>Modules</a></li>*@
                        <li><a href='@Url.Content("~/Home/Services")'> Services</a></li>
                        <li class="active">Job Sheet List</li>
                    </ol>
                </section>
            </div>
            <div class="box-tools">
                @*@if(UTILITY.HasRight("CustomerInvoiceAdd", "Action"))
                    {
                        @Html.EncodedActionLink("<i class='fa fa-plus' aria-hidden='true'></i>&nbsp;New Invoice",
                        "CustomerInvoice", "Invoice",
                        new { invoiceNo = "", documentNo = "" }, new { @class = "btn btn-default btn-sm" })
                    }*@
            <button class="btn btn-default btn-sm" onclick="location.href='@Url.Content("~/Services/CreateJob")'">
                <span>
                    <i class='fa fa-plus' aria-hidden='true'></i>
                </span>
                Add
            </button>


            </div>
        </div>
        <div class="box-body">
            <div class="form-group">

                <div class="row">
                    <div class="col-md-1">

                    </div>
                    <div class="col-md-1">
                        Service No
                    </div>
                    <div class="col-md-2">
                        @Html.TextBox("Service No", "", new { @class = "form-control input-sm" })
                    </div>
                    <div class="col-md-1">
                        Invoice No
                    </div>
                    <div class="col-md-2">
                        @Html.TextBox("Invoice No", "", new { @class = "form-control input-sm" })
                    </div>

                </div>
            </div>
            <div class="form-group">
                <div class="row">



                    <div class="col-md-1">

                    </div>
                    <div class="col-md-1">
                        Mobile No
                    </div>
                    <div class="col-md-2">
                        @Html.TextBox("Mobile No", "", new { @class = "form-control input-sm" })
                    </div>
                    <div class="col-md-1">
                        Date
                    </div>
                    <div class="col-md-2">
                        @Html.TextBox("Date", "", new { @class = "form-control input-sm datepicker" })
                    </div>
                    <div class="col-md-1">
                        <button class="btn btn-default btn-sm">Search</button>
                    </div>
                </div>
            </div>
            <div class="row" style="margin-top:10px;">
                <div class="col-md-12">
                    <table class="table table-striped" id="tblCustomerInvoices">
                        <thead>
                            <tr>
                                <th>Service No</th>
                                <th>Date</th>
                                <th>Customer</th>
                                <th>Engineer</th>
                                <th>Status</th>
                            </tr>
                        </thead>
                        <tbody>
                           
                                <tr>
                                    <td>123</td>
                                    <td>12/11/2017</td>
                                    <td>kusumanjali</td>
                                    <td>kusuma</td>
                                    <td>
                                        <label class="control-label">
                                            <span class="label label-primary" style="font-weight:bold;font-size:14px;">Done</span>
                                        </label>
                                    </td>
                                   </tr>
                            <tr>
                                <td>125</td>
                                <td>12/12/2018</td>
                                <td>Prasad</td>
                                <td>kusuma</td>
                                <td>
                                    <label class="control-label">
                                        <span class="label label-info" style="font-weight:bold;font-size:14px;">In Progress</span>
                                    </label>
                                </td>
                            </tr>
                            <tr>
                                <td>124</td>
                                <td>12/11/2018</td>
                                <td>Bhushan</td>
                                <td>kusuma</td>
                                <td>
                                    <label class="control-label">
                                        <span class="label label-danger" style="font-weight:bold;font-size:14px;">Blocked</span>
                                    </label>
                                </td>
                            </tr>
                            <tr>
                                <td>127</td>
                                <td>13/11/2018</td>
                                <td>sharma</td>
                                <td>kusuma</td>
                                <td>
                                    <label class="control-label">
                                        <span class="label label-warning" style="font-weight:bold;font-size:14px;">Needs Review</span>
                                    </label>
                                </td>
                            </tr>
                            <tr>
                                <td>127</td>
                                <td>13/11/2018</td>
                                <td>Mahesh</td>
                                <td>Kethu</td>
                                <td>
                                    <label class="control-label">
                                        <span class="label label-default" style="font-weight:bold;font-size:14px;">N/A</span>
                                    </label>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>
<script src="@Url.Content("~/Scripts/moment.js")"></script>
<script src="@Url.Content("~/Scripts/bootstrap-datetimepicker.js")"></script>
<script src="@Url.Content("~/Scripts/select2.min.js")"></script>
<script type="text/javascript">
    $(function(){
        $('.datepicker').datetimepicker({
        useCurrent: false,
        format: 'DD/MM/YYYY',
        inline: false
        });
    });

</script>

